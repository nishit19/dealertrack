@model DealerTrack.Models.ImportCSV.DealGridViewModel

<script src="~/Scripts/ImportCSV/DealGrid.js"></script>

@if (!Model.DealDetails.Any())
{
    <div class="alert alert-info">
        No records found
    </div>
}
else
{
    <table id="tblDeal" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>Deal Number</th>
                <th>Customer Name</th>
                <th>Dealership Name</th>
                <th>Vehicle</th>
                <th>Price</th>
                <th>Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var dealDetail in Model.DealDetails)
            {
                <tr>
                    <td>@dealDetail.DealerNumber</td>
                    <td>@dealDetail.CustomerName</td>
                    <td>@dealDetail.DealerShipName</td>
                    <td>@dealDetail.Vehicle</td>
                    <td>@dealDetail.PriceFormatted</td>
                    <td>@dealDetail.DateFormatted</td>
                </tr>
            }
        </tbody>
    </table>
}
